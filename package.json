{
  "name": "scripturajs21",
  "version": "0.0.1",
  "description": "A framework for Node.js",
  "author": "Olivier Chavarin",
  "license": "MIT",
  "main": "./bin/www",
  "private": true,
  "dependencies": {
    "argon2": "^0.28.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "date-easter": "^0.2.5",
    "debug": "^4.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-urlrewrite": "^1.4.0",
    "express-useragent": "^1.0.13",
    "helmet": "^4.5.0",
    "http-errors": "~1.8.0",
    "i18n-iso-countries": "^6.0.0",
    "ip": "^1.1.5",
    "luxon": "^2.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.11.0",
    "pug": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "suncalc": "^1.8.0",
    "uglify-es": "^3.3.9",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "autoprefixer-stylus": "^1.0.0",
    "browser-sync": "^2.26.14",
    "eslint": "^7.25.0",
    "jest": "^27.2.0",
    "postcss": "^8.2.15",
    "purgecss": "^4.0.3",
    "stylus": "^0.55.0"
  },
  "scripts": {
    "start": "NODE_ENV=production nodemon ./bin/www",
    "dev": "yarn js | yarn css | yarn cssexp | yarn cssprint | yarn bs | yarn go",
    "js": "uglifyjs public/scripts/development/main.js -o public/scripts/main.js -c -m",
    "test": "jest",
    "css": "stylus -u autoprefixer-stylus -w public/styles/development/main.styl -o public/styles/main.css -c -m",
    "cssexp": "stylus -u autoprefixer-stylus -w public/styles/development/main.styl -o public/styles/expanded/main.css",
    "cssprint": "stylus -u autoprefixer-stylus -w public/styles/development/print.styl -o public/styles/print.css -c -m",
    "bs": "browser-sync start --proxy 'localhost:3000' --port 9001 --reload-debounce 2000 --files 'public' 'views' --no-notify",
    "go": "nodemon ./bin/www"
  }
}
